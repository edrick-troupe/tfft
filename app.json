{
	"expo": {
		"name": "tfft",
		"slug": "tfft",
		"scheme": "tfft",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#F5E9CF"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#F5E9CF"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
